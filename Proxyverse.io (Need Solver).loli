  HEADER "Pragma: no-cache" 
  HEADER "Accept: */*" 

#Api_Key PARSE "<SOURCE>" LR "id=\"apiKey\" class=\"input block w-full border mt-2 enabled\" enabled value=\"" "\">" CreateEmpty=FALSE -> CAP "Api Key" 

